package Compre_Examples;
class valueException1 extends Exception 
{
	valueException1()
	{
		System.out.println("Small Number Exception");
	}
}
public class Main {
	public static void main(String args[])
	{
		int a= Integer.parseInt(args[0]);
		try 
		{
			NumberChecker(a);
		}
		catch(valueException1 ve)
		{
			System.out.println(ve);
		}
	}
	static void NumberChecker(int a) throws valueException1
	{
		if(a<100)
		{
			throw new valueException1();	
		}
		else
		{
			System.out.println("The number is:"+a);
		}
	}

}
